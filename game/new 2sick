function executePromScript($scriptfile , $var) {
	
	global $Main;
	
	// Load specified script
	$scriptfile = file_get_contents("data/scripts/".$scriptfile);
	
	// Convert it to PHP
	$scriptfile = strreplace("var[", "$customvariable[");
	$scriptfile = strreplace("var_", "$customvariable");
	$scriptfile = strreplace("output[", "$output[");
	$scriptfile = strreplace('"', "/'");
	
	// Execute PHP
	
	// Update player variables
	foreach($customvariable AS $name => $value){
		updatePlayerVarDB($name,$value);
	}
	
	return $output;
}

