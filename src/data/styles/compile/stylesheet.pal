/*  Promathius Style Sheet
    --------------------------------------------------------------
	Style name:		Default
	
	This is the default style for Promathius.
	Note that all comments and spaces will be ommited.
    --------------------------------------------------------------
*/

html, body {
	 background: #171717 url(images/gui/background.gif) top center no-repeat;
	 height: 100%;
	 margin: 0 0 0px 0;
	 color: #D0D0D0;
	 font-size: 100%;
	 /* cursor: url(images/cursor.cur), url(images/cursor/cursor.cur), default; */
}
/* Disable Mozilla Firefox selection indicators 
button::-moz-focus-inner,
input[type="reset"]::-moz-focus-inner,
input[type="button"]::-moz-focus-inner,
input[type="submit"]::-moz-focus-inner,
input[type="file"] > input[type="button"]::-moz-focus-inner {
border: none;
} */
font, th, td {
	font-size:11px;
	font-family: Verdana, Arial;
	/** Safari font fix **/
	text-shadow: #000000 0 0 0px;
}
#factiondescription {
	padding: 18px;
	border-right: solid 1px #222222;
	border-bottom: solid 1px #222222;
	background: #262626 url(images/gui/shadows/inner_top.png) top left repeat-x;
}
td.regionselect {
	border-right: solid 1px #222222;
	border-bottom: solid 1px #222222;
	background: #262626 url(images/gui/shadows/inner_top.png) top left repeat-x;
}
div.turnbase {
	width: 420px;
	text-align:center;
margin-left: auto;
margin-right: auto;
	padding: 18px;
	border-right: solid 1px #222222;
	border-bottom: solid 1px #222222;
	background: #161616 url(images/gui/shadows/inner_top.png) top left repeat-x;
}
#turnaction, #turnresult, #turnorder, div.transparent, img.transparent {
	opacity:0;
	-moz-opacity: 0;
	-khtml-opacity: 0;
	filter: alpha(opacity=0);
 }
img {
	border: none;
}
table {
	border: #000000 0px solid;
	padding: 0px;
	/* border-collapse: collapse; */
	text-align:left;
}


/* Links */
a, a:visited {
	color: #C49400;
	text-decoration: none;
	outline: none;
}
a:hover {
	color: #cad3e1;
}
a:active {
	color: #FFFFFF;
}
a.new {
	color: #C49400;
}
a.mally {
	color: #9FBFDF;
}
a.mdead {
	color: red;
}
a.mnormal {
	color: white;
}
a.undecorated {
	color: white;
}
a.undecorated:hover {
	color: #000000;
	background: white;
}
a.proflinkdk {
	color: #000000;
	text-decoration: underline;
}

