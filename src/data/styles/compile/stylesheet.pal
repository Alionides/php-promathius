/*--------------------------------------------------
  REQUIRED to hide the non-active tab content.
  But do not hide them in the print stylesheet!
  --------------------------------------------------*/
.tabberlive .tabbertabhide {
	display:none;
}
/*--------------------------------------------------
  ul.tabbernav = the tab navigation list
  li.tabberactive = the active tab
  --------------------------------------------------*/
ul.tabbernav {
	margin:0;
	padding: 0px;
	padding-bottom: 11px;
	text-align:center;
}
ul.tabbernav li {
	list-style: none;
	margin: 0;
	padding: 0px;
	display: inline;
}
ul.tabbernav li a {
	text-decoration: none;
}
ul.tabbernav li a:hover {
	text-decoration: underline;
	color: #FFFFFF;
	cursor: hand;
}
ul.tabbernav li.tabberactive a {
	color: #D0D0D0;
	font-weight: bold;
}
ul.tabbernav li.tabberactive a:hover {
	text-decoration: none;
	cursor: default;
}
/*--------------------------------------------------
  .tabbertab = the tab content
  Add style only after the tabber interface is set up (.tabberlive)
  --------------------------------------------------*/
.tabberlive .tabbertab {
	padding-left:24px;  
	height:190px;
	border-top:0;/* If you don't want the tab size changing whenever a tab is changed
    you can set a fixed height */

 /* height:200px; */

 /* If you set a fix height set overflow to auto and you will get a
    scrollbar when necessary */

 /* overflow:auto; */
}
/* If desired, hide the heading since a heading is provided by the tab */
.tabberlive .tabbertab h2 {
	display:none;
}
.tabberlive .tabbertab h3 {
	display:none;
}

/* Tabbed status tables */
.tabberstatus {
	width:348px;
}

/* Styled Select Boxes */
select.replaced {
	width: 1px;
	position: relative;
	left: -999em;
}
ul.selectReplacement {
	border: solid 1px #C49400;
	position: absolute;
	margin: 0;
	padding: 0;
	text-align: left;
	width: 200px;
}
ul.selectReplacement:hover {
	border: solid 1px #FFFFFF;
	margin: 0;
	padding: 0;
}
ul.selectReplacement li {
	background: #212121;
	color: #fff;
	cursor: pointer;
	display: none;
	line-height: 1.6em;
	list-style: none;
	margin: 0;
	padding: 1px 5px;
}
ul.selectOpen li {
	display: block;
}
ul.selectFocused {
	border: solid 1px #FFFFFF;
}
ul.selectReplacement li.selected {
	color: #fff;
	background: url(images/gui/inputbg.gif) top left no-repeat;
	display: block;
}
ul.selectOpen li.selected {
	background: #3d3d3d url(images/gui/selectedbg.gif) top left repeat-x;
	background-position: 0 0px;
	display: block;
}
ul.selectOpen li:hover,  ul.selectOpen li.hover,  ul.selectOpen li.selected:hover {
	background: #565656 url(images/gui/selectedbg.gif) top left repeat-x;
	background-position: 0 -24px;
	color: #fff;
}
